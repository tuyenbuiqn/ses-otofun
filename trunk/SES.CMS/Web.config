      <add name="RewriteCategoryx" virtualUrl="^~/(.*)-(.*).aspx" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/Category.aspx?CategoryID=$2" ignoreCase="true" />
      
